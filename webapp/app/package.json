{
  "name": "cloudshare-react",
  "version": "1.0.0",
  "description": "Cloudshare front-end refactor with react.",
  "main": "index.js",
  "scripts": {
    "clean": "rm ../static.bak ../templates_dist.bak -rf && rm ../static.prod ../templates_dist.prod -rf",
    "release": "mkdir -p ../static ../templates_dist && mv ../static ../static.bak && mv ../static.prod ../static && mv ../templates_dist ../templates_dist.bak && mv ../templates_dist.prod ../templates_dist",
    "dev": "./node_modules/.bin/webpack-dev-server",
    "build": "./node_modules/.bin/webpack --progress --display-error-details",
    "build:production": "npm run clean && npm run build && npm run release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cloudshare",
    "react"
  ],
  "author": "kabessC",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.8.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.5",
    "echarts": "^3.2.3",
    "marked": "^0.3.6",
    "moment": "^2.17.1",
    "react": "^15.5.0",
    "react-addons-css-transition-group": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "jsdom": "^9.9.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "postcss-loader": "^1.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
